# Example environment variables for the Policy Query API
# Copy this file to .env and fill in your values

# Google API Keys for Gemini models (use individual keys for better deployment compatibility)
GOOGLE_API_KEY_1=your_first_google_api_key_here
GOOGLE_API_KEY_2=your_second_google_api_key_here  
GOOGLE_API_KEY_3=your_third_google_api_key_here

# Fallback Google API Key (used if individual keys are not set)
GOOGLE_API_KEY=your_google_api_key_here

# Optional API key for authentication (leave empty to disable auth)
API_KEY=YOUR_KEY_HERE

# Server Configuration for Render deployment
PORT=10000
WEB_CONCURRENCY=1

# CORS Configuration (comma-separated list of allowed origins)
ALLOWED_ORIGINS=*

# Application Environment
ENVIRONMENT=production
